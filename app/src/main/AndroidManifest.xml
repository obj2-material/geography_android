<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.edu.unq.cpi.geography">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ar.edu.unq.cpi.geography.view.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ar.edu.unq.cpi.geography.view.population.PoblacionPorPaisActivity"
            android:parentActivityName="ar.edu.unq.cpi.geography.view.main.MainActivity" />
        <activity
            android:name="ar.edu.unq.cpi.geography.view.population.ListaDePaisesActivity"
            android:parentActivityName="ar.edu.unq.cpi.geography.view.main.MainActivity" />
        <activity
            android:name="ar.edu.unq.cpi.geography.view.population.DetalleDePaisActivity"
            android:parentActivityName="ar.edu.unq.cpi.geography.view.population.ListaDePaisesActivity" />
    </application>

</manifest>